      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>

  <link name="karcher_urdf__front_wheel">
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="0.02917" ixy="0" ixz="0" iyy="0.02917" iyz="0" izz="0.025"/>
