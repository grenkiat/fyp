  <link name="karcher_camera">
    <collision>
      <origin  rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="5 5 5"/>
      </geometry>
    </collision>

    <visual>
      <origin  rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="5 5 5"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="0.1" />
      <origin  rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
