    <child link="karcher_camera"/>
    <origin xyz="0.2 0 0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0" />
  </joint>

  <link name="karcher_urdf__chassis">
    <inertial>
      <mass value="72.5"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <inertia ixx="322.8" ixy="0" ixz="0" iyy="221.43" iyz="0" izz="290.15"/>
    </inertial>

    <collision name="karcher_urdf__chassis_collision_collision">
      <origin rpy="0  0  0" xyz="0    0    0.09"/>
