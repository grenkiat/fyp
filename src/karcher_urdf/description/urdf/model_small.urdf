    </inertial>

    <collision name="karcher_urdf__rear_left_wheel_collision_collision">
      <origin rpy="0.31415 -0.15707  0.31415" xyz="0  0  0"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
    </collision>

